<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <RadialGradientBrush x:Key="DarkRadialBrush">
        <GradientStopCollection>
            <GradientStop Color="Blue"></GradientStop>
            <GradientStop Offset="0.6" Color="Black"></GradientStop>
        </GradientStopCollection>
    </RadialGradientBrush>
    <!-- darkSolidBrush.Color.R < 150 && darkSolidBrush.Color.G < 150 && darkSolidBrush.Color.B < 150 -->
    <SolidColorBrush x:Key="DarkSolidBrush" Color="#000"></SolidColorBrush>
    <SolidColorBrush x:Key="FontBrush" Color="#FFF"></SolidColorBrush>
</ResourceDictionary>